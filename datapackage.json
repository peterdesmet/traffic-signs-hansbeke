{
    "name": "traffic-signs-hansbeke",
    "title": "Traffic signs of Hansbeke, Nevele, Belgium",
    "description": "Geospatial dataset of all traffic signs of Hansbeke, Nevele, Belgium. See https://github.com/peterdesmet/traffic-signs-hansbeke for more information.",
    "keywords": [
        "traffic signs",
        "geospatial",
        "Hansbeke",
        "Nevele",
        "Belgium"
    ],
    "datapackage_version": "1.0-beta.10",
    "licenses": [
        {
            "type": "cc-zero",
            "url": "http://creativecommons.org/publicdomain/zero/1.0/"
        }
    ],
    "sources": [
        {
            "name": "Peter Desmet",
            "web": "http://peterdesmet.com"
        }
    ],
    "maintainers": [
        {
            "name": "Peter Desmet",
            "web": "http://peterdesmet.com"        
        }
    ],
    "base": "https://github.com/peterdesmet/traffic-signs-hansbeke/tree/master/",
    "resources": [
        {
            "path": "data/traffic_signs_hansbeke.csv",
            "name": "traffic-signs-hansbeke-as-csv",
            "format": "csv",
            "schema": {
                "fields": [
                    {
                        "name": "cartodb_id",
                        "description": "Record ID. Generated by CartoDB.",
                        "type": "integer"
                    },
                    {
                        "name": "text_on_sign",
                        "description": "Text on the traffic sign.",
                        "type": "string"
                    },
                    {
                        "name": "code",
                        "description": "Belgian traffic sign code.",
                        "url": "http://wiki.openstreetmap.org/wiki/Road_signs_in_Belgium",
                        "type": "string"
                    },
                    {
                        "name": "created_at",
                        "description": "Date at which records was created. Generated by CartoDB.",
                        "type": "date"
                    },
                    {
                        "name": "updated_at",
                        "description": "Date at which record was updated. Generated by CartoDB.",
                        "type": "date"
                    },
                    {
                        "name": "verified",
                        "description": "True if the existence of the traffic sign has been verified on location.",
                        "type": "boolean"
                    },
                    {
                        "name": "street",
                        "description": "Street in which the traffic sign was found.",
                        "type": "string"
                    },
                    {
                        "name": "facing",
                        "description": "Wind direction in which the traffic sign is facing.",
                        "type": "string",
                        "values": ["N","E","S","W"]
                    },
                    {
                        "name": "text_below_sign",
                        "description": "Text on the sub sign.",
                        "type": "string"
                    },
                    {
                        "name": "code_prefix",
                        "description": "First character of Belgian traffic sign code, indicating the type of traffic sign.",
                        "type": "string",
                        "values": ["A","B","C","D","E","F",""]
                    },
                    {
                        "name": "longitude",
                        "description": "Decimal longitude of traffic sign, in webmercator.",
                        "type": "number"
                    },
                    {
                        "name": "latitude",
                        "description": "Decimal latitude of traffic sign, in webmercator.",
                        "type": "number"
                    },
                    {
                        "name": "geojson",
                        "description": "Point coordinates of the traffic sign.",
                        "type": "geometry",
                        "format": "geojson"
                    }
                ]
            }
        },
        {
            "path": "data/traffic_signs_hansbeke.geojson",
            "name": "traffic-signs-hansbeke-as-geojson",
            "format": "geojson"
        }
    ]
}
