{
    "name": "traffic-signs-hansbeke",
    "title": "Traffic signs of Hansbeke, Belgium",
    "description": "Geospatial dataset of all traffic signs of Hansbeke, Nevele, Belgium",
    "keywords": [
        "traffic signs",
        "geospatial",
        "Hansbeke",
        "Nevele",
        "Belgium"
    ],
    "last_modified": "2013-08-02",
    "datapackage_version": "1.0-beta",
    "licenses": [
        {
            "id": "cc-zero",
            "url": "http://creativecommons.org/publicdomain/zero/1.0/"
        }
    ],
    "sources": [
        {
            "name": "Peter Desmet",
            "web": "http://peterdesmet.com"
        }
    ],
    "maintainers": [
        {
            "name": "Peter Desmet",
            "web": "http://peterdesmet.com"        
        }
    ],
    "resources": [
        {
            "path": "data/traffic_signs_hansbeke.csv",
            "name": "traffic_signs_hansbeke.csv",
            "format": "csv",
            "schema": {
                "fields": [
                    {
                        "id": "cartodb_id",
                        "description": "Record ID, generated by CartoDB",
                        "type": "number"
                    },
                    {
                        "id": "text_on_sign",
                        "description": "Text on the traffic sign",
                        "type": "string"
                    },
                    {
                        "id": "code",
                        "description": "Belgian traffic sign code",
                        "url": "http://wiki.openstreetmap.org/wiki/Road_signs_in_Belgium",
                        "type": "string"
                    },
                    {
                        "id": "created_at",
                        "description": "Record created at date, generated by CartoDB",
                        "type": "date"
                    },
                    {
                        "id": "updated_at",
                        "description": "Record updated at data, generated by CartoDB",
                        "type": "date"
                    },
                    {
                        "id": "verified",
                        "description": "True if the existence of the traffic sign has been verified on location",
                        "type": "boolean"
                    },
                    {
                        "id": "street",
                        "description": "Street on which the traffic sign occurs",
                        "type": "string"
                    },
                    {
                        "id": "facing",
                        "description": "Wind direction in which the traffic sign is facing",
                        "type": "string",
                        "values": ["N","E","S","W"]
                    },
                    {
                        "id": "text_below_sign",
                        "description": "Text on the sub sign",
                        "type": "string"
                    },
                    {
                        "id": "code_prefix",
                        "description": "First character of Belgian traffic sign code, indicating the type of traffic sign",
                        "type": "string",
                        "values": ["A","B","C","D","E","F",""]
                    },
                    {
                        "id": "longitude",
                        "description": "Decimal longitude of traffic sign, in webmercator",
                        "type": "number"
                    },
                    {
                        "id": "latitude",
                        "description": "Decimal latitude of traffic sign, in webmercator",
                        "type": "number"
                    },
                    {
                        "id": "geojson",
                        "description": "Point coordinates of the traffic sign, rounded to 5 decimals",
                        "type": "geometry",
                        "format": "geojson"
                    }
                ]
            }
        }
    ]
}